# AutoML Frontend Environment Variables

# API Configuration
REACT_APP_API_URL=http://localhost:5000
REACT_APP_API_VERSION=v1

# Environment
NODE_ENV=development

# Application Configuration
REACT_APP_APP_NAME=AutoML Studio
REACT_APP_APP_VERSION=1.0.0

# Feature Flags
REACT_APP_ENABLE_ANALYTICS=false
REACT_APP_ENABLE_ERROR_REPORTING=false

# Development Settings
REACT_APP_DEBUG_MODE=true
REACT_APP_LOG_LEVEL=info

# Optional: Authentication (if added later)
# REACT_APP_AUTH_ENABLED=false
# REACT_APP_AUTH_PROVIDER=local

# Optional: Theme Settings
# REACT_APP_DEFAULT_THEME=dark
# REACT_APP_THEME_SWITCHING_ENABLED=true